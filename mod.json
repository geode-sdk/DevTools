{
	"geode": "5.0.0-beta.4",
	"version": "v1.13.0",
	"gd": {
		"win": "2.2081",
		"android": "*",
		"mac": "2.2081",
		"ios": "2.2081"
	},
	"id": "geode.devtools",
	"name": "DevTools",
	"developer": "Geode Team",
	"description": "Developer tools for Geode",
	"api": { "include": ["include/*.hpp"] },
	"links": {
		"source": "https://github.com/geode-sdk/DevTools"
	},
	"issues": {
		"url": "https://github.com/geode-sdk/DevTools/issues",
		"info": "If you encounter an issue using DevTools, please report it to the GitHub issues page."
	},
	"settings": {
		"should-use-gd-window": {
			"type": "bool",
			"name": "Use GD window",
			"description": "Determines if the DevTools should use a custom GD window or not. Required to disable for some exotic configurations (MacOS Wine).",
			"default": true
		},
		"open-bind": {
			"type": "keybind",
			"name": "DevTools Keybind",
			"descriptions": "The keybind to open DevTools",
			"default": {
				"win": "F11",
				"mac": ["F10", "F11"],
				"android": "F11",
				"ios": "F11"
			}
		}
	},
	"resources": {
		"sprites": [
			"resources/*.png"
		]
	}
}
